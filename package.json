{
  "scripts": {
    "test": "cypress open",
    "test-reporter-chrome": "cypress run  --browser chrome --reporter cypress-multi-reporters --reporter-options configFile=config-report-chrome.json --spec",
    "test-reporter-edge": "cypress run  --browser edge --reporter cypress-multi-reporters --reporter-options configFile=config-report-edge.json --spec",
    "test-reporter-firefox": "cypress run  --browser firefox --reporter cypress-multi-reporters --reporter-options configFile=config-report-firefox.json --spec"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "cypress": "^13.13.3",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-plugin-api": "^2.11.2",
    "cypress-plugin-steps": "^1.1.1",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
